{
  "name": "whats-up-darkness",
  "productName": "What's up darkness",
  "version": "1.1.3",
  "description": "dark themed whatsapp web client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:win-ia32": "electron-packager . whats-up-darkness --overwrite --platform=win32 --arch=ia32 --icon=./assets/img/ico/icon.ico --asar --prune --app-copyright=MIT --out=release-builds --win32metadata.CompanyName=tncga --win32metadata.ProductName=\"What's up darkness?\" --executable-name=wupd",
    "build:win-x64": "electron-packager . whats-up-darkness --overwrite --platform=win32 --arch=x64 --icon=./assets/img/ico/icon.ico --asar --prune --app-copyright=MIT --out=release-builds --win32metadata.CompanyName=tncga --win32metadata.ProductName=\"What's up darkness?\" --executable-name=wupd",
    "build:linux-ia32": "electron-packager ./ whats-up-darkness --overwrite --asar --prune -platform=linux --arch=ia32 --icon=./assets/img/png/icon_normal.png --out=release-builds --executable-name=wupd; sudo chmod +x ./release-builds/whats-up-darkness-linux-x64/wupd",
    "build:linux-x64": "electron-packager ./ whats-up-darkness --overwrite --asar --prune -platform=linux --arch=x64 --icon=./assets/img/png/icon_normal.png --out=release-builds --executable-name=wupd; sudo chmod +x ./release-builds/whats-up-darkness-linux-x64/wupd"
  },
  "repository": "https://github.com/tncga/whats-up-darkness",
  "author": "tncga",
  "license": "MIT",
  "devDependencies": {
    "electron": "^3.0.0-beta.6"
  },
  "dependencies": {
    "usercss-meta": "^0.6.1"
  }
}
