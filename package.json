{
	"name": "whats-up-darkness",
	"productName": "WhatsApp",
	"version": "2.0.8",
	"description": "An Electron based unofficial dark-themed WhatsApp client",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"reinstall": "npm install --force",
		"bump:patch": "VERSION=$(npm version patch); VERSION=$(echo $VERSION | cut -c 2-); echo '{\"version\": \"'$VERSION'\"}' > assets/userdata/info.json;",
		"bump:minor": "VERSION=$(npm version minor); VERSION=$(echo $VERSION | cut -c 2-); echo '{\"version\": \"'$VERSION'\"}' > assets/userdata/info.json;",
		"bump:major": "VERSION=$(npm version major); VERSION=$(echo $VERSION | cut -c 2-); echo '{\"version\": \"'$VERSION'\"}' > assets/userdata/info.json;",
		"build:linux-x64": "electron-packager ./ whats-up-darkness --overwrite -platform=linux --arch=x64 --asar --ignore=\"^/assets|/install.sh\" --extra-resource=assets/ --out=release-builds --executable-name=wupd; chmod +x ./release-builds/whats-up-darkness-linux-x64/wupd",
		"build:linux-ia32": "electron-packager ./ whats-up-darkness --overwrite -platform=linux --arch=ia32 --asar --ignore=\"^/assets|/install.sh\" --extra-resource=assets/ --out=release-builds --executable-name=wupd; chmod +x ./release-builds/whats-up-darkness-linux-ia32/wupd",
		"build:win-ia32": "electron-packager . whats-up-darkness --overwrite --platform=win32 --arch=ia32 --asar --ignore=\"^/assets|/install.sh\" --extra-resource=assets/ --icon=./assets/img/icon.ico --app-copyright=MIT --out=release-builds --win32metadata.CompanyName=tncga --win32metadata.ProductName=\"What's up darkness?\" --executable-name=wupd",
		"build:win-x64": "electron-packager . whats-up-darkness --overwrite --platform=win32 --arch=x64 --asar --ignore=\"^/assets|/install.sh\" --extra-resource=assets/ --icon=./assets/img/icon.ico --app-copyright=MIT --out=release-builds --win32metadata.CompanyName=tncga --win32metadata.ProductName=\"What's up darkness?\" --executable-name=wupd"
	},
	"author": {
		"name": "tncga",
		"email": "qokberk_akdeniz@hotmail.com"
	},
	"repository": "https://github.com/tncga/whats-up-darkness",
	"license": "MIT",
	"dependencies": {
		"electron-is-dev": "^1.1.0",
		"fs-extra": "^7.0.1",
		"module-alias": "^2.2.0",
		"usercss-meta": "^0.9.0"
	},
	"_moduleAliases": {
		"@root": ".",
		"@constants": "./constants.js",
		"@app_main": "app/main",
		"@app_updater": "app/updater/main.js",
		"@app_about": "app/about/window.js",
		"@app_settings": "app/settings"
	},
	"devDependencies": {
		"electron": "^4.2.3"
	},
	"constants": {
		"userAgentFirefoxVersion": "66.0"
	}
}
